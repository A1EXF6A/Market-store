# Configuración para tests con Supabase
# Copia este archivo a .env.test y completa los valores

# Supabase Database Connection
SUPABASE_DB_HOST=aws-0-us-west-1.pooler.supabase.com
SUPABASE_DB_PORT=6543
SUPABASE_DB_USER=postgres
SUPABASE_DB_PASSWORD=tu_password_supabase
SUPABASE_DB_NAME=postgres

# O usando la URL completa de conexión (alternativa)
SUPABASE_DATABASE_URL=postgresql://postgres:[PASSWORD]@[HOST]:[PORT]/postgres

# JWT Secret para tests
JWT_SECRET=test_jwt_secret_key

# Node environment
NODE_ENV=test